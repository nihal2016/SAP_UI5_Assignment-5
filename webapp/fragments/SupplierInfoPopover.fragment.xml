<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core" xmlns:f="sap.ui.layout.form">
	<Popover title="Supplier Inforamtion">
		<footer>
			<OverflowToolbar>
				<ToolbarSpacer/>
				<Button text="Close" press="handleActionPress"/>
			</OverflowToolbar>
		</footer>
		<f:SimpleForm editable="false" layout="ResponsiveGridLayout" binding="{}">
			<f:content>
				<Label text="{i18n>SupplierInfoSupplierIDText}"/>
				<Text text="{SupplierID}"/>
				<Label text="{i18n>SupplierInfoNameText}"/>
				<Text text="{ContactName}"/>
				<Label text="{i18n>SupplierInfoCompanyNameText}"/>
				<Text text="{CompanyName}"/>
				<Label text="{i18n>SupplierInfoContactTitleText}"/>
				<Text text="{ContactTitle}"/>
				<Label text="{i18n>SupplierInfoContactTitleText}"/>
				<Text text="{Phone}"/>
				<Text text="{i18n>SupplierInfoContactTitleText}"/>
				<Label text="{i18n>SupplierInfoStreetText}"/>
				<Text text="{Address}"/>
				<Label text="{i18n>SupplierInfoPostalCodeCityText}"/>
				<Text text="{PostalCode}, {City}"/>
				<Label text="{i18n>SupplierInfoCountryText}"/>
				<Text text="{Country}"/>
			</f:content>
		</f:SimpleForm>
	</Popover>
</core:FragmentDefinition>